<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_page">
    <sys_ui_page action="INSERT_OR_UPDATE">
        <category>general</category>
        <client_script/>
        <description/>
        <direct>false</direct>
        <endpoint>sn_blog_management_preview_blog.do</endpoint>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="null" xmlns:g="null" xmlns:j2="jelly:core" xmlns:g2="glide">
	<!--${sysparm_title}
	${sysparm_preview_token} -->
	<g2:evaluate var="jvar_gr" object="true">
		var gr = new GlideRecord('sn_blog_management_blog_post');
		<!-- gr.addQuery('title', '${sysparm_title}'); -->
		gr.addQuery('preview_token', '${sysparm_preview_token}');
		gr.query();
		gr;
	</g2:evaluate>
	
	<table border="1" cellspacing="2">
		<j2:if test="${jvar_gr.hasNext()}">
			<tr>
				<td>Title</td>
				<td>Author</td>
				<td>State</td>
			</tr>
				<j2:while test="${jvar_gr.next()}">
					<tr>
						<td><g2:no_escape>${jvar_gr.getValue('title')}</g2:no_escape></td>
						<td>${jvar_gr.getValue('author')}</td>
						<td>${jvar_gr.getValue('state')}</td>
					</tr>
					<tr><td colspan="3"><g2:no_escape>${jvar_gr.getValue('body')}</g2:no_escape></td></tr>
				</j2:while>
		</j2:if>
	</table>
</j:jelly>
]]></html>
