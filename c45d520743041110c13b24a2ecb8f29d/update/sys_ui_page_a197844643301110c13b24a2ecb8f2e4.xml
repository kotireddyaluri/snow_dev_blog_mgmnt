<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_page">
    <sys_ui_page action="INSERT_OR_UPDATE">
        <category>general</category>
        <client_script/>
        <description/>
        <direct>false</direct>
        <endpoint>sn_blog_management_blog_list.do</endpoint>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="null" xmlns:g="null" xmlns:j2="jelly:core" xmlns:g2="glide">
	<style>
		table, td{
		padding: 3px;
		}
	</style>
	<h3>Recent 10 Blogs</h3>
	<g2:evaluate var="jvar_gr" object="true">
		var gr = new GlideRecord('sn_blog_management_blog_post');
		gr.query();
		gr.setLimit(10);
		gr;
	</g2:evaluate>
	<table border="1" cellspacing="2">
		<j2:if test="${jvar_gr.hasNext()}">
			<tr>
				<td>Title</td>
				<td>Author</td>
			</tr>
			<g2:no_escape>
				<j2:while test="${jvar_gr.next()}">
					<tr>
						<td><a href='/' target="_blank">${jvar_gr.getValue('title')}</a></td>
							<td>${jvar_gr.getValue('author')}</td>
					</tr>
				</j2:while>
			</g2:no_escape>
		</j2:if>
	</table>
</j:jelly>]]></html>
        <name>blog_list</name>
